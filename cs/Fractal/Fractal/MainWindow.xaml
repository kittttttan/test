<Window x:Class="Fractal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="503" Width="610" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23" />
            <RowDefinition Height="400*" />
            <RowDefinition Height="37" />
            <RowDefinition Height="23" />
        </Grid.RowDefinitions>
        <Image Grid.Row="1" Name="image1" Stretch="Fill" />
        <StatusBar Grid.Row="3" Name="statusBar1" Height="23" VerticalAlignment="Bottom" />
        <Menu Height="23" Name="menu1" VerticalAlignment="Top" />
        <Button Content="C#" Grid.Row="2" HorizontalAlignment="Left" Margin="12,0,0,6" Name="button1" Width="75" Height="23" VerticalAlignment="Bottom" Click="button_CS_Click" />
        <TextBlock Grid.Row="3" Height="23" HorizontalAlignment="Left" Margin="12,0,0,0" Name="textBlock1" Text="TextBlock" VerticalAlignment="Top" />
        <Button Content="C Dll" Grid.Row="2" HorizontalAlignment="Left" Margin="93,0,0,6" Name="button2" Width="75" Height="23" VerticalAlignment="Bottom" Click="button_C_Click" />
        <Button Content="Clear" Grid.Row="2" HorizontalAlignment="Left" Margin="174,0,0,6" Name="button3" Width="75" Height="23" VerticalAlignment="Bottom" Click="button_Clear_Click" />
    </Grid>
</Window>
